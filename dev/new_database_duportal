---------- New table to store 


-- sample_data_set -> id, payload, type




CREATE TABLE duportal.sample_data_set (
	sdv_id serial NOT NULL,
	type text NOT NULL,
	payload json NOT NULL,
	created timestamptz DEFAULT ('now'::text)::timestamp(3) with time zone NOT NULL,
	PRIMARY KEY ("sdv_id")
);


